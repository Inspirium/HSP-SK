{
  "name": "Inspirium/HSP",
  "description": "Hollistic Solutions for Publishers",
  "keywords": ["framework", "laravel"],
  "license": "GPLv3",
  "type": "project",
  "require": {
    "php": ">=7.0.0",
    "laravel/framework": "5.4.*",
    "laravel/tinker": "~1.0"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "jeroen-g/laravel-packager": "^1.5",
    "laravel/homestead": "^5.1",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.7"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
            "Inspirium\\SKTemplate\\": "packages/Inspirium/SKTemplate/src",
      "Inspirium\\UserManagement\\": "packages/Inspirium/UserManagement/src",
      "Inspirium\\HumanResources\\": "packages/Inspirium/HumanResources/src",
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
            "Inspirium\\SKTemplate\\": "packages/Inspirium/SKTemplate/src",
      "Inspirium\\UserManagement\\": "packages/Inspirium/UserManagement/src",
      "Inspirium\\HumanResources\\": "packages/Inspirium/HumanResources/src",
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan optimize"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  }
}
